{
    "name": "Verifeyed Prototype",
    "version": "1.0",
    "description": "Fact checking extension for Firefox",
    "manifest_version": 2,
    "author": "Verifeyed Team",
    "icons": {
        "48": "icons/visual.png",
        "96": "icons/visual.png"
    },
    "browser_action":{
        "default_popup": "index.html",
        "default_title": "Verifeyed",
        "default_icon" : "icons/visual.png"
    },
    "permissions": ["tabs","contextMenus", "menus", "activeTab"],
    "background": {
        "scripts": ["script.js", "axios.js"]
    }
}